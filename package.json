  "scripts": {
    "start": "node ./bin/www",
    "build": "npx parcel build lib.js -d public -o lib.js",
    "build-production": "NODE_ENV=production npx parcel build lib.js -d public -o lib.js --no-source-maps",
    "watch": "npx parcel watch lib.js -d public",
    "dev": "npx nodemon --inspect",
    "docker-rebuild": "docker-compose up -d --build --force-recreate --no-deps file-upload",
    "docker-clear": "docker exec -it file-upload node bin/clearAllFiles.js",
    "docker-size": "docker exec -it file-upload node bin/folderSize.js",
    "docker-list": "docker exec -it file-upload node bin/listFilesInDb.js"
  },
